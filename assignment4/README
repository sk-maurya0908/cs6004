~/openj9-openjdk-jdk8/build/assignment4$ javac -cp .:sootclasses-trunk-jar-with-dependencies.jar *.java
~/openj9-openjdk-jdk8/build/assignment4$ ../linux-x86_64-normal-server-release/images/j2sdk-image/bin/javac testcases/*.java

